@article{okamotoBeatingStandardQuantum2008,
	title = {Beating the standard quantum limit: phase super-sensitivity of\${\textbackslash}less\$i\${\textbackslash}greater\${N}\${\textbackslash}less\$/i\${\textbackslash}greater\$-photon interferometers},
	volume = {10},
	issn = {1367-2630},
	shorttitle = {Beating the standard quantum limit},
	url = {https://doi.org/10.1088/1367-2630/10/7/073033},
	doi = {10.1088/1367-2630/10/7/073033},
	abstract = {Quantum metrology promises greater sensitivity for optical phase measurements than could ever be achieved classically. Here, we present a theory of the phase sensitivity for the general case where the detection probability is given by an N photon interference fringe. We find that the phase sensitivity has a complicated dependence on both the intrinsic efficiency of detection η and the interference fringe visibility V. Most importantly, the phase that gives maximum phase sensitivity is in general not the same as the phase at which the slope of the interference fringe is a maximum, as has previously been assumed. We determine the parameter range where quantum enhanced sensitivity can be achieved. In order to illustrate these theoretical results, we perform a four-photon experiment with η=3/4 and V=82±6\% (an extension of our previous work (Nagata et al 2007 Science 316 726)) and find a phase sensitivity 1.3 times greater than the standard quantum limit at a phase different to that which gives maximum slope of the interference fringe.},
	language = {en},
	number = {7},
	urldate = {2022-08-05},
	journal = {New Journal of Physics},
	author = {Okamoto, Ryo and Hofmann, Holger F. and Nagata, Tomohisa and O'Brien, Jeremy L. and Sasaki, Keiji and Takeuchi, Shigeki},
	month = jul,
	year = {2008},
	note = {Publisher: IOP Publishing},
	keywords = {fock state, interferometer, super-resolution, super-sensitivity},
	pages = {073033},
	file = {okamotoBeatingStandardQuantum2008.pdf:/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/okamotoBeatingStandardQuantum2008.pdf:application/pdf},
}


@misc{branczykHongOuMandelInterference2017,
	title = {Hong-{Ou}-{Mandel} {Interference}},
	url = {http://arxiv.org/abs/1711.00080},
	doi = {10.48550/arXiv.1711.00080},
	abstract = {This article is a detailed introduction to Hong-Ou-Mandel (HOM) interference, in which two photons interfere on a beamsplitter in a way that depends on the photons' distinguishability. We begin by considering distinguishability in the polarization degree of freedom. We then consider spectral distinguishability, and show explicitly how to calculate the HOM dip for three interesting cases: 1) photons with arbitrary spectral distributions, 2) spectrally entangled photons, and 3) spectrally mixed photons.},
	urldate = {2022-07-04},
	publisher = {arXiv},
	author = {Brańczyk, Agata M.},
	month = oct,
	year = {2017},
	note = {arXiv:1711.00080 [quant-ph]},
	keywords = {Quantum Physics},
	file = {arXiv.org Snapshot:/home/dkeathley/Zotero/storage/QTT8TR3A/1711.html:text/html;branczykHongOuMandelInterference2017.pdf:/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/branczykHongOuMandelInterference2017.pdf:application/pdf},

	file = {nagataBeatingStandardQuantum2007.pdf:/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/nagataBeatingStandardQuantum2007.pdf:application/pdf;Snapshot:/home/dkeathley/Zotero/storage/A68IV3I9/726.html:text/html},
